<!DOCTYPE html>
<html>
<head>
  <title>TWS</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <script type="text/javascript">
    <% if logged_in? %>
    window.currentUser = <%= render("api/users/user.json.jbuilder",
      user: current_user).html_safe %>
    <% end %>
  </script>

</head>
<body>
  <div id="content"></div>

<%= yield %>

</body>
</html>
